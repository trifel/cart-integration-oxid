{
  "name": "shopgate/cart-integration-oxid",
  "description": "Integrates the Oxid shopping cart solution with Shopgate",
  "homepage": "https://github.com/shopgate/cart-integration-oxid",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Shopgate",
      "email": "interfaces@shopgate.com"
    }
  ],
  "config": {
    "platform": {
      "php": "5.6"
    },
    "vendor-dir": "src/modules/shopgate/vendor"
  },
  "require": {
    "php": ">=5.6",
    "shopgate/cart-integration-sdk" : "dev-developer"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*"
  },
  "autoload": {
    "psr-4": {
      "unit\\": "tests/unit/",
      "tests\\" : "tests/"
    }
  },
  "repositories": {
    "shopgate/cart-integration-sdk": {
      "type": "git",
      "url": "https://github.com/trifel/cart-integration-sdk.git"
    }
  }
}
